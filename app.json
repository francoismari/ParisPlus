{
  "expo": {
    "name": "Paris+",
    "slug": "ParisPlus",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.francoismari.dtap",
      "buildNumber": "1.2.0",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Access to location is required to be able to find toilets nearby."
      }
    },
    "android": {
      "package": "com.francoismari.dtap",
      "versionCode": 12,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}